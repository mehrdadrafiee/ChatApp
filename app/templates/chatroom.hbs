<h1>Invitations</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |users|}}
    <tr>
      <th>{{users.id}}</th>
      <td>{{users.username}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
